{
    "version": 2,
    "builds": [
      {
        "src": "backend/app/main.py",
        "use": "@vercel/python"
      },
      {
        "src": "frontend/build/web/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/app/main.py"
      },
      {
        "src": "/manifest.json",
        "dest": "frontend/build/web/manifest.json"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/build/web/$1"
      }
    ]
  }